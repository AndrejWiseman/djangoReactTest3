{
    "builds": [{
        "src": "djangoReactTest3/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
          "distDir": "frontend/build/static"
        }
      }
    ],

    "routes": [
        {
            "src": "/frontend/build/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "djangoReactTest3/wsgi.py"
        }
    ],
    "outputDirectory": "frontend/build/static"
}